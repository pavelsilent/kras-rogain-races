{
  "name": "krsk-rogain-results-front",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "generate:api": "openapi-generator-cli generate -i ../build/openapi/openapi.json -g typescript-angular -c openapi-generator-config.json -o ./src/app/api -t ./templates "
